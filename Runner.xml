<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Practice Suite" parallel="classes" thread-count="4">
    <listeners>
        <listener class-name="TestListeners.Listener"></listener>
    </listeners>
    <test name="Login Test">
        <classes>
            <class name="LoginTest.LoginTest"></class>
        </classes>
    </test> <!-- Test -->
    <test name="Product and cart tests">
        <classes>
            <class name="ProductsTests.ProductInfoTest"></class>
            <class name="ProductsTests.CartTests"></class>
        </classes>
    </test>
</suite> <!-- Suite -->